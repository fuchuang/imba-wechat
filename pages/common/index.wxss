  background: #F6F6F6;
  height: 100%;
}
/* index btn */
.btnTitle{
  position: absolute;
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  font-size: large;
  color: #7c7c7c;
  background: white;
}
.MYREbtnBG{
  width:100%;height:100%;position:fixed;top:0;left:0;
}
.MYREbtnPos{
  width:260rpx;border:solid 2px #51d6ff;position:absolute;left:254rpx;background: white;
}
.MYREbtnItem{
  width:260rpx;height:50rpx;align-items:center;font-size:medium;color:#51d6ff;

}
/* 内容的css */
.scrollItemBG{
  border-top-right-radius: 20rpx;
