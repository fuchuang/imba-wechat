<view class='noticeContainBG flexRow flexRowCenterAndAround' style='top:{{gradeCardTop+50}}rpx;height:{{noticeHeight+10}}rpx;'>
  <view style='width:640rpx;height:{{noticeHeight}}rpx;background: white;border-radius: 20rpx;'>
    <view class='noticeContainTitle flexColumn inManyLine '>
      <view>{{noticeMessageDetauil.notiTitle}}</view>
      <view style='width:400rpx;border-bottom:solid 2px #37fbbf;height:20rpx;'></view>
    </view>
    <scroll-view  style='height:{{noticeHeight-260}}rpx;width:640rpx;margin-top:20rpx;' scroll-y>
      <view style='width:100%;align-items: center;' class='flexColumn'>
        <view class='noticeContainText'>{{noticeMessageDetauil.content}}
        </view> 
       <view class='noticeContainFoot flexRow'>
          <view>
            <view>{{noticeMessageDetauil.title}}</view>
            <view>2018.12.22</view>
          </view>
          <view class='flexRow'>
          <view style='background:url("https://www.gdutrex.xyz/imba/noticeContainDowanload.png");background-size:100% 100%;width:42rpx;height:42rpx;'/>
          <text>下载附件</text>
          </view>
       </view>
      </view>
    </scroll-view>
  </view>
</view>
<view class='noticeContainStar' style='background:url("https://www.gdutrex.xyz/imba/noticeContainStarOff.png");background-size:100% 100%;top:{{gradeCardTop+100}}rpx;'/>
